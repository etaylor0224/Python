# Input a list of student scores
student_scores = input().split()
for n in range(0, len(student_scores)):
  student_scores[n] = int(student_scores[n])

# Write your code below this row ðŸ‘‡

max_student = student_scores[0]
for max_score in range(len(student_scores)):
  if student_scores[max_score]> max_student:
    max_student = student_scores[max_score]

print(f"The highest score in the class is: {max_student}")
